{
  "hosting": {
    "site": "biurokms-9ce7b",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/*","headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ],
        "destination": "/index.html"
      }
    ]
  }
}
